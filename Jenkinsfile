pipeline {
    agent any
    stages {
        stage('Checkout') {
            echo 'Getting source code...'
            checkout scm
        }

        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}

